{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console":"off",
        "linebreak-style":0,
        "quotes": [
            "error"
        ],
        "comma-spacing": ["error", { "before":false,"after":true}]
    }
}
